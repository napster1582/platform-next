{
  "name": "cms",
  "version": "0.1.0-alpha",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts tsc -w & ts-node-dev src/server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts payload build",
    "build:server": "tsc",
    "build": "npm run copyfiles && npm run build:payload && npm run build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload-config.js node dist/server.js",
    "seed": "cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts ts-node src/seed.ts",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload-config.ts payload generate:types",
    "lint": "eslint .",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "payload": "^1.6.9",
    "styled-components": "6.0.0-rc.3"
  },
  "devDependencies": {
    "@payloadcms/plugin-form-builder": "^1.0.15",
    "@payloadcms/plugin-nested-docs": "^1.0.4",
    "@payloadcms/plugin-search": "^1.0.0",
    "@payloadcms/plugin-seo": "^1.0.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-router-dom": "^5.3.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "process": "^0.11.10",
    "dotenv": "^16.1.4",
    "express": "^4.17.1",
    "payload": "^1.7.2",
    "payload-plugin-password-protection": "^0.0.2",
    "react": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "dotenv": "^16.1.4",
    "react": "^18.2.0",
    "react-router-dom": "^5.3.4"
  }
}
