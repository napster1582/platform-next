    "@popperjs/core": "^2.11.8",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.22.6",
    "@types/qs": "^6.9.7",
    "cms": "^1.8.1",
    "date-fns": "^2.30.0",
