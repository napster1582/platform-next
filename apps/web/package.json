    "@jinen/web-http": "workspace:^",
    "@neoconfetti/svelte": "^1.0.0",
    "@iconify/tailwind": "^0.1.3",
    "@playwright/test": "^1.28.1",
    "@popperjs/core": "^2.11.8",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.2.4",
    "@sveltejs/kit": "^1.20.4",
    "@types/qs": "^6.9.7",
    "cms": "^1.8.1",
    "escape-html": "^1.0.3",
    "qs": "^6.11.2",
    "slate": "^0.94.1",
    "svelte": "^4.0.0",
