{
  "name": "@jinen/helpers",
  "version": "0.1.0-alpha",
  "main": "src/index.ts",
  "exports": {
    ".": {
      "serve": {
        "types": "./dist/index.js",
        "require": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "default": {
        "types": "./src/index.ts",
        "require": "./src/index.ts",
        "import": "./src/index.ts"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@jinen/eslint-config": "workspace:^",
    "@jinen/tsconfig": "workspace:^",
    "@jinen/annotations": "workspace:^"
  }
}
